<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%- include('header.ejs')%>

    <h1>Displaying All Notes</h1>

    <div class="">
        <% notes.forEach((obj) => { %>
            <% obj.notes.forEach((note) => { %>
                <span>NotesId : <%= note._id%></span> <br>
                <span>Title : <%= note.title%></span> <br>
                <span>Content : <%= note.content%></span> <br>
                <span>Edit Notes : <a href="/admin/editNote/<%= note._id%>">edit</a> </span> <br>
                <span>Delete Notes : <a href="/admin/deleteNote/<%= note._id%>">delete</a> </span> 
                <br>
                <br>
            <% }) %>
            <br>
        <% }) %>
    </div>


</body>
</html>